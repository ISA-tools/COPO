{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "ISA factor value schema",
    "description": "JSON-schema representing a factor value in the ISA model",
    "type": "object",
    "properties": {
        "factor": {
             "$ref": "factor_schema.json#"
        },
        "value": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "ontologyAnnotation": {
                    "$ref": "ontology_annotation_schema.json#"
                }
            }
        }
    }
}